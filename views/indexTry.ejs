        var map;
            function initMap() {
                i=7;
                map = new google.maps.Map(document.getElementById('map'), {
                    center: { lat: 12.971, lng: 79.1589 },
                    zoom: 7
                });
                var id = setInterval(function () {
                    i+=1;
                    map.setZoom(i);
                    if (i==15){
                        clearInterval(id);
                    }
                },1000);

                
                var marker = new google.maps.Marker({
                    position: { lat: 12.971, lng: 79.1589 },
                    map: map,
                    title: 'My Location'
                });
            }
